pipeline {

agent any

stages {

stage(‘Checkout’) {

steps {

checkout([$class: ‘GitSCM’, branches: [[name: ‘*/main’]], extensions: [], userRemoteConfigs: [[url: ‘git@github.com:Hamritha02/Task-Expleo.git‘]]])

}

}

stage (“terraform init”) {

steps {

sh (‘terraform init’)

}

}

stage (“terraform plan”) {

steps {

sh (‘terraform plan’)

}

}
stage (“terraform apply”) {

steps {

sh (‘terraform apply’)

}

}
